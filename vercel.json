{
  "version": 2,
  "builds": [
    { "src": "index.mjs", "use": "@vercel/node" }, 
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/quizzes", "dest": "index.mjs" }, 
    { "src": "/", "dest": "index.html" },
    { "src": "/(.*)", "dest": "index.html" }
  ]
}